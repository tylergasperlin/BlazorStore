<div style="background-color:lightgreen">
    <h3> Los Child Component </h3>
    <h5 style="color:red"> @Title </h5>
    <GrandChildComponent/>
    <h1>@Message</h1>
</div>

@code{
    [Parameter]
    public string Title {get; set;}

    [CascadingParameter (Name = "message")]
    public string Message { get; set; }
}